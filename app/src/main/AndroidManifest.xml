<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.doubanapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".TestApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".listactivity.BooklistActivity"
            android:label="书籍搜索结果" />
        <activity
            android:name=".listactivity.MovielistActivity"
            android:label="电影搜索结果" />
        <activity
            android:name=".listactivity.MusiclistActivity"
            android:label="音乐搜索结果" />
        <activity
            android:name=".detailactivity.BookDetailActivity"
            android:label="书籍详情" />
        <activity
            android:name=".detailactivity.MovieDetailActivity"
            android:label="电影详情" />
        <activity
            android:name=".detailactivity.MusicDetailActivity"
            android:label="音乐详情" />
    </application>

</manifest>